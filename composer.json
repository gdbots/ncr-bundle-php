{
  "name": "gdbots/ncr-bundle",
  "homepage": "https://github.com/gdbots/ncr-bundle-php",
  "description": "Symfony3 bundle that integrates gdbots/ncr library.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/ncr": "~0.1",
    "gdbots/pbjx-bundle": "~0.1",
    "symfony/symfony": "~3.2"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php": "~3.21",
    "ruflin/elastica": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\NcrBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\NcrBundle\\": "tests"
    }
  }
}
